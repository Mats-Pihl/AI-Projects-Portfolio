     ]
    }
   ],
   "source": [
    "#Word2vec\n",
    "\n",
    "#tokenize messages for word2vec\n",
    "from nltk.tokenize import word_tokenize\n",
    "import nltk\n",
    "nltk.download('punkt')\n",
    "\n",
    "df['tokens'] = df['message'].apply(lambda x: word_tokenize(x.lower()))\n",
    "\n",
    "#train word2vec model\n",
    "from gensim.models import Word2Vec\n",
    "\n",
    "w2v_model = Word2Vec(sentences=df['tokens'], vector_size=100, window=5, min_count=1, workers=4)\n",
    "\n",
    "#average word vectors per message\n",
    "import numpy as np\n",
    "\n",
    "def get_avg_vector(tokens):\n",
    "    vectors = [w2v_model.wv[word] for word in tokens if word in w2v_model.wv]\n",
    "    return np.mean(vectors, axis=0) if vectors else np.zeros(100)\n",
    "\n",
    "df['w2v_vector'] = df['tokens'].apply(get_avg_vector)\n",
    "\n",
    "#split word2vec features for training\n",
    "X_w2v = np.vstack(df['w2v_vector'].values)\n",
    "y_w2v = df['label'].values\n",
    "\n",
    "X_train_w2v, X_test_w2v, y_train_w2v, y_test_w2v = train_test_split(X_w2v, y_w2v, test_size=0.2, random_state=42)\n"
   ]
  },
