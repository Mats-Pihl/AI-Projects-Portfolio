    "print(\"SVM - Binary\\n\", classification_report(y_test, svm_bin.predict(X_test_bin)))\n",
    "\n",
    "#term frequency\n",
    "svm_tf = LinearSVC()\n",
    "svm_tf.fit(X_train_tf, y_train)\n",
    "print(\"SVM - Term Frequency\\n\", classification_report(y_test, svm_tf.predict(X_test_tf)))\n",
    "\n",
    "#tf-idf\n",
    "svm_tfidf = LinearSVC()\n",
    "svm_tfidf.fit(X_train_tfidf, y_train)\n",
    "print(\"SVM - TF-IDF\\n\", classification_report(y_test, svm_tfidf.predict(X_test_tfidf)))\n",
    "\n",
    "#word2vec\n",
    "svm_w2v = LinearSVC()\n",
    "svm_w2v.fit(X_train_w2v, y_train_w2v)\n",
    "print(\"SVM - Word2Vec\\n\", classification_report(y_test_w2v, svm_w2v.predict(X_test_w2v)))"
   ]
